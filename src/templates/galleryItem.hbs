{{#each this}}

<div class="photo-card">
    {{!-- <div class="photo"><img src="{{webformatURL}}" alt="picture" width="350px" /></div> --}}
    <div class="photo"><img src="{{webformatURL}}" class="img-thumbnail" alt="picture"></div>

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</div>

{{/each}}